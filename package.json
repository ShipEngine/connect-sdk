{
  "name": "@shipengine/ipaas",
  "version": "0.0.1",
  "description": "Core functionality for creating, testing, and building an IPaaS integration.",
  "keywords": [],
  "author": {
    "name": "James Messinger",
    "url": "https://jamesmessinger.com"
  },
  "license": "MIT",
  "homepage": "https://jstools.dev/shipengine-ipaas",
  "repository": {
    "type": "git",
    "url": "https://github.com/ShipEngine/shipengine-ipaas.git"
  },
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib"
  ],
  "scripts": {
    "clean": "shx rm -rf .nyc_output coverage lib",
    "lint": "npm run lint:typescript && npm run lint:javascript",
    "lint:typescript": "tslint -p tsconfig.json",
    "lint:javascript": "eslint test",
    "build": "tsc && shx cp -r src/template lib/",
    "watch": "tsc --watch",
    "test": "mocha && npm run lint",
    "coverage": "nyc node_modules/mocha/bin/mocha",
    "upgrade": "npm-check -u && npm audit fix",
    "bump": "bump --tag --push --all",
    "release": "npm run upgrade && npm run clean && npm run build && npm test && npm run bump"
  },
  "engines": {
    "node": ">=10"
  },
  "devDependencies": {
    "@jsdevtools/eslint-config-modular": "^8.0.0",
    "@jsdevtools/tslint-modular": "^2.0.2",
    "@jsdevtools/version-bump-prompt": "^6.0.2",
    "@types/chai": "^4.1.7",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.9.0",
    "chai": "^4.2.0",
    "eslint": "^6.8.0",
    "mocha": "^7.1.0",
    "npm-check": "^5.9.0",
    "nyc": "^15.0.0",
    "shx": "^0.3.2",
    "source-map-support": "^0.5.12",
    "tslint": "^6.0.0",
    "typescript": "^3.5.1",
    "typescript-tslint-plugin": "^0.5.0"
  },
  "dependencies": {
    "@types/fs-extra": "^8.1.0",
    "@types/inquirer": "^6.5.0",
    "fs-extra": "^8.1.0",
    "inquirer": "^7.1.0"
  }
}
